        "eslint-plugin-babel": "^5.3.0",
        "mocha": "^6.1.4",
        "selenium-webdriver": "^4.0.0-alpha.4",
        "sleep": "^6.1.0",
        "wdio": "^3.0.3",
        "wdio-chromedriver-service": "^5.0.2",
        "wdio-mocha-framework": "^0.6.4",
        "webdriverio": "^5.11.6"
    },
    "dependencies": {
        "allure-commandline": "^2.9.0"
    }
